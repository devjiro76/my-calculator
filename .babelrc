{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7", "ie >= 8"]
        }
      }
    ]
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    "transform-async-to-generator",
    "transform-object-rest-spread",
    "transform-es3-property-literals",
    "transform-es3-member-expression-literals"
  ]
}
